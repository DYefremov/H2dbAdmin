<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="parent" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="by.post.control.ui.SettingsPaneController">
   <children>
      <ListView fx:id="settingsList" onKeyReleased="#onSettingsKeyReleased" onMouseClicked="#onSettingsMouseClicked" prefWidth="200.0">
          <items>
              <FXCollections fx:factory="observableArrayList">
                  <String fx:value="Database" />
                  <String fx:value="User interface" />
              </FXCollections>
          </items>
      </ListView>
   </children>
</HBox>
